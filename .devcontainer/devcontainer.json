{
    "name": "AtCoder",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "remoteUser": "vscode",
    "customizations": {
        "vscode": {
            "extensions": [
                "rust-lang.rust-analyzer",
                "ms-vscode.cpptools",
                "vadimcn.vscode-lldb",
                "fill-labs.dependi",
                "tamasfe.even-better-toml",
                "oderwat.indent-rainbow",
                "MS-CEINTL.vscode-language-pack-ja",
                "mechatroner.rainbow-csv",
                "tonybaloney.vscode-pets"
            ]
        }
    },
    "runArgs": ["--name", "atcoder"]
}